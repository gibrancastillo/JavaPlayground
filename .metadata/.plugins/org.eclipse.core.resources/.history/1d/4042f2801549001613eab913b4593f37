package com.ccc.threads;

public class ProcessExecution {
	
	public void testMethod() {
		for(int i = 1; i <= 10; i++) {
			System.out.println("i value: " + i);
		}
	}
	
	public static void main(String[] args) {
		ProcessExecution pe1 = new ProcessExecution();
		pe1.testMethod();
		pe1.testMethod();
		pe1.testMethod();
	}
}